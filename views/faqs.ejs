<div class="accordion" id="faqAccordion">
  <% for (let i = 0; i < faqs.length; i++) { %>
    <div class="accordion-item">
      <h2 class="accordion-header" id="faqHeading<%= i %>">
        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#faqCollapse<%= i %>" aria-controls="faqCollapse<%= i %>">
          <%= faqs[i].question %>
        </button>
      </h2>
      <div id="faqCollapse<%= i %>" class="accordion-collapse collapse" aria-labelledby="faqHeading<%= i %>">
        <div class="accordion-body">
          <%= faqs[i].answer %>
        </div>
      </div>
    </div>
  <% } %>
</div>
